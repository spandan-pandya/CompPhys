    "### N dimensional hypersphere"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "d6aceb6f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Defining a general function for an N dimensional volume\n",
    "def general_unit_volume(dim, iterations): #takes dimensions and the number of montecarlo sweeps required\n",
    "    count_in_sphere = 0 #number of points inside the generalised space \n",
    "\n",
    "    for points in range(iterations): #number of iterations\n",
    "        dim_arr=np.zeros(dim) #stores the value of the random values in x y z whatever dimensions\n",
    "        for i in range(dim):\n",
    "            dim_arr[i]=2*np.random.uniform()-1 #creates N-dimension random numbers and stores it \n",
    "\n",
    "        distance=np.sqrt(np.sum(dim_arr**2))# calculates the general form of the radius \n",
    "        if distance < 1.0: # if the point is in circle \n",
    "            count_in_sphere += 1 #then add to it \n",
    "\n",
    "    return np.power(2.0, dim) * (count_in_sphere / iterations) \n",
    "           # the ratio multiplied by the area of a 2L square (hence L=1 leaves us with 2^N)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "f517f5b4",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Using this function for different dimensions\n",
    "dimensions=np.arange(2,15,1)\n",
    "V_d=np.zeros(len(dimensions))\n",
    "for i in range(len(dimensions)):\n",
    "    V_d[i]=general_unit_volume(dimensions[i], 10_00_000)"
   ]
