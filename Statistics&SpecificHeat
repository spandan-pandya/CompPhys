   "source": [
    "#Error in calculation\n",
    "I_exact=np.pi*(np.sqrt(6)-1)\n",
    "I_MC=Area\n",
    "error=(abs(I_exact-I_MC)/I_exact)*100\n",
    "print('The area using Monte carlo method is ',str(I_MC))\n",
    "print('The percentage error in our calculation is:',np.round(error,3),'%')"
   ]
