    "plt.legend(bbox_to_anchor=(1, 1))\n",
    "plt.ylim(0.25,0.65)\n",
    "plt.xlabel('N')\n",
    "plt.ylabel(r'$\\langle n \\rangle$')\n",
    "plt.show()"
   ]
