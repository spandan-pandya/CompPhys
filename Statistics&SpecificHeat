   "source": [
    "def c(T): #defining temperature\n",
    "    epsilon=1\n",
    "    return np.exp(epsilon/T)/(1+np.exp(epsilon/T))**2*(epsilon**2/T**2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "f7611c10",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Creating data\n",
    "N=10000 # for 10000 points\n",
    "tolerance=10**(-2) # this was chosen because the other smaller values, the function was blowing\n",
    "temperature=np.linspace(tolerance,5,N) \n",
    "capacities=np.zeros(N)\n",
    "for i in range(N):\n",
    "    capacities[i]=c(temperature[i])"
   ]
  },
